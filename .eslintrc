{
    "parser": "babel-eslint",
    "env" : {
        "es6": true,
        "browser" : true,
        "node": true
    },
    "rules" : {
        "semi":"off",
        "linebreak-style": 0,
        "require-jsdoc" : 0,
        "indent": 0,
        "no-tabs": 0,
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "space-before-blocks": 0,
        "padded-blocks": ["error", { "blocks": "never" }]
    },


    "extends": ["eslint:recommended", "google"]
}
